<?xml version="1.0" encoding="utf-8"?>
<EventChannel
	xmlns="urn:schemas-cosylab-com:DDSEvent:1.0"
	xmlns:opendds="http://www.omg.org/dds"
	xmlns:ev="urn:schemas-cosylab-com:EventChannel:1.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="urn:schemas-cosylab-com:DDSEvent:1.0 DDSEvent.xsd">
	<!--  <ev:Events>
      <ev:e MaxProcessTime="2.0" Name=""/>
	</ev:Events>-->
	<opendds:dds>
    <opendds:qos_profile name="profileTest">
      <opendds:publisher_qos>
        <opendds:partition>
          <opendds:name>
            <opendds:element>Message</opendds:element>
          </opendds:name>
        </opendds:partition>
      </opendds:publisher_qos>
      <opendds:subscriber_qos>
        <opendds:partition>
          <opendds:name>
            <opendds:element>Message</opendds:element>
          </opendds:name>
        </opendds:partition>
      </opendds:subscriber_qos>
      <opendds:datareader_qos>
				<opendds:durability>
            <opendds:kind>VOLATILE_DURABILITY_QOS</opendds:kind>
				</opendds:durability>
      </opendds:datareader_qos>
      <opendds:datawriter_qos>
           <opendds:history>
             <opendds:kind>KEEP_ALL_HISTORY_QOS</opendds:kind>
             <opendds:depth>15</opendds:depth>
        </opendds:history>
      </opendds:datawriter_qos>
      <opendds:topic_qos>
           <opendds:durability>
                <opendds:kind>VOLATILE_DURABILITY_QOS</opendds:kind>
           </opendds:durability>
           <opendds:durability_service>
                <opendds:service_cleanup_delay>
                     <opendds:sec>0</opendds:sec>
                     <opendds:nanosec>0</opendds:nanosec>
                </opendds:service_cleanup_delay>
                <opendds:history_kind>KEEP_LAST_HISTORY_QOS</opendds:history_kind>
                <opendds:history_depth>1</opendds:history_depth>
                <opendds:max_samples>LENGTH_UNLIMITED</opendds:max_samples>
                <opendds:max_instances>LENGTH_UNLIMITED</opendds:max_instances>
                <opendds:max_samples_per_instance>LENGTH_UNLIMITED</opendds:max_samples_per_instance>
           </opendds:durability_service>
           <opendds:deadline>
                <opendds:period>
                     <opendds:sec>DURATION_INFINITE_SEC</opendds:sec>
                     <opendds:nanosec>DURATION_INFINITE_NSEC</opendds:nanosec>
                </opendds:period>
           </opendds:deadline>
           <opendds:latency_budget>
                <opendds:duration>
                     <opendds:sec>0</opendds:sec>
                     <opendds:nanosec>0</opendds:nanosec>
                </opendds:duration>
           </opendds:latency_budget>
           <opendds:liveliness>
                <opendds:kind>AUTOMATIC_LIVELINESS_QOS</opendds:kind>
                <opendds:lease_duration>
                     <opendds:sec>DURATION_INFINITE_SEC</opendds:sec>
                     <opendds:nanosec>DURATION_INFINITE_NSEC</opendds:nanosec>
                </opendds:lease_duration>
           </opendds:liveliness>
           <opendds:reliability>
                <opendds:kind>BEST_EFFORT_RELIABILITY_QOS</opendds:kind>
                <opendds:max_blocking_time>
                     <opendds:sec>0</opendds:sec>
                     <opendds:nanosec>100000000</opendds:nanosec>
                     </opendds:max_blocking_time>
           </opendds:reliability>
           <opendds:destination_order>
                <opendds:kind>BY_RECEPTION_TIMESTAMP_DESTINATIONORDER_QOS</opendds:kind>
           </opendds:destination_order>
           <opendds:history>
                <opendds:kind>KEEP_LAST_HISTORY_QOS</opendds:kind>
                <opendds:depth>1</opendds:depth>
           </opendds:history>
           <opendds:resource_limits>
                <opendds:max_samples>LENGTH_UNLIMITED</opendds:max_samples>
                <opendds:max_instances>LENGTH_UNLIMITED</opendds:max_instances>
                <opendds:max_samples_per_instance>LENGTH_UNLIMITED</opendds:max_samples_per_instance>
           </opendds:resource_limits>
           <opendds:transport_priority>
                <opendds:value>0</opendds:value>
           </opendds:transport_priority>
           <opendds:lifespan>
                <opendds:duration>
                     <opendds:sec>DURATION_INFINITE_SEC</opendds:sec>
                     <opendds:nanosec>DURATION_INFINITE_NSEC</opendds:nanosec>
                </opendds:duration>
           </opendds:lifespan>
           <opendds:ownership>
                <opendds:kind>SHARED_OWNERSHIP_QOS</opendds:kind>
           </opendds:ownership>
      </opendds:topic_qos>
		</opendds:qos_profile>
	</opendds:dds>
</EventChannel>

